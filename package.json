{
  "name": "zographia",
  "version": "0.1.0",
  "author": "Ziphil",
  "description": "Versatile component library",
  "homepage": "https://github.com/Ziphil/Zographia",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "develop": "storybook dev -p 6006",
    "build": "webpack --mode production --config webpack-config.ts && tsc-alias",
    "build:storybook": "storybook build --output-dir ./docs",
    "lint": "eslint source/**/*.ts source/**/*.tsx",
    "lint:fix": "eslint --fix source/**/*.ts source/**/*.tsx"
  },
  "eslintConfig": {
    "extends": [
      "ziphil",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "webpack-*.ts"
    ],
    "rules": {
      "no-multi-spaces": [
        "warn",
        {
          "ignoreEOLComments": true
        }
      ],
      "react/no-unknown-property": [
        "warn",
        {
          "ignore": [
            "styleName"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.4",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/pro-regular-svg-icons": "^6.5.1",
    "@fortawesome/pro-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fortawesome/sharp-regular-svg-icons": "^6.5.1",
    "@fortawesome/sharp-solid-svg-icons": "^6.5.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "markdown-it": "^13.0.2",
    "react-helmet": "^6.1.0",
    "react-is": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-overflow-list": "^0.5.0",
    "react-use": "^17.4.2",
    "remark-gfm": "^4.0.0",
    "simplebar-react": "^3.2.4",
    "ts-essentials": "^9.4.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@react-docgen/cli": "^2.0.1",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-webpack5": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "@types/color": "^3.0.6",
    "@types/css-tree": "^2.3.4",
    "@types/react": "^18.2.41",
    "@types/react-css-modules": "^4.6.8",
    "@types/react-dom": "^18.2.17",
    "@types/react-helmet": "^6.1.11",
    "@types/react-is": "^18.2.4",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-tree": "^2.3.1",
    "eslint": "^8.17.0",
    "eslint-config-ziphil": "^1.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.6",
    "react": "^18.2.0",
