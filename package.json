    "ts-dedent": "^2.2.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0",
    "yaml-flat-loader": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
